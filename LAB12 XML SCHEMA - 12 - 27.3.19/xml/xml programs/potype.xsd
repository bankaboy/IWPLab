<schema xmlns="http://www.w3.org/2001/XMLSchema" xmlns:ns1="www.abc.com/purchase" targetNamespace="www.abc.com/purchase">
	<simpleType name="prstring">
		<restriction base="string">
			<pattern value="[p]{1}\d{3}"/>
		</restriction>
	</simpleType>
	<simpleType name="orstring">
		<restriction base="string">
			<pattern value="[o]{1}\d{3}"/>
		</restriction>
	</simpleType>
</schema>
